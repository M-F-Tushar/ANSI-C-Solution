//Write a C program to print the current system date.
#include <stdio.h>
#include <time.h>

int main() {
    time_t t;
    struct tm *current_time;

    time(&t);
    current_time = localtime(&t);

    printf("Current Date: %02d-%02d-%d\n",
           current_time->tm_mday,
           current_time->tm_mon + 1,
           current_time->tm_year + 1900);

    return 0;
}
/*Explanation:

Uses time.h to get system time.
Uses localtime() to extract day, month, and year*/

